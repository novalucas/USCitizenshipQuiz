    <!-- QUIZ Question 1 selection -->
    <string name="quiz_1_question">Who was the first U.S. President?</string>
    <string name="quiz_1_answer_1">Abraham Lincoln</string>
    <string name="quiz_1_answer_2">George Washington</string>
    <string name="quiz_1_answer_3">John Adams</string>
    <string name="quiz_1_answer_4">Thomas Jefferson</string>
    <!-- QUIZ Question 2 selection -->
    <string name="quiz_2_question">When was the Constitution written?</string>
    <string name="quiz_2_answer_1">1878</string>
    <string name="quiz_2_answer_2">1816</string>
    <string name="quiz_2_answer_3">1789</string>
    <string name="quiz_2_answer_4">1787</string>
    <!-- QUIZ Question 3 selection -->
    <string name="quiz_3_question">What are the two parts of the U.S. Congress?</string>
    <string name="quiz_3_answer_1">Senate</string>
    <string name="quiz_3_answer_2">Judiciary</string>
    <string name="quiz_3_answer_3">House (of Representatives)</string>
    <string name="quiz_3_answer_4">Oval Office</string>
    <!-- QUIZ Question 4 selection -->
    <string name="quiz_4_question">How many justices are on the Supreme Court?</string>
    <string name="quiz_4_answer">9</string>
    <string name="quiz_4_hint">0</string>
    <!-- QUIZ Question 5 selection -->
    <string name="quiz_5_question">Who did the United States fight in World War II?</string>
    <string name="quiz_5_answer_1">Russia</string>
    <string name="quiz_5_answer_2">Japan</string>
    <string name="quiz_5_answer_3">China</string>
    <string name="quiz_5_answer_4">Germany</string>
    <string name="quiz_5_answer_5">Italy</string>
    <!-- QUIZ Question 6 selection -->
    <string name="quiz_6_question">If both the President and the Vice President can no longer serve, who becomes President?</string>
    <string name="quiz_6_answer_1">The Chief of Staff</string>
    <string name="quiz_6_answer_2">The Defense Secretary</string>
    <string name="quiz_6_answer_3">The Speaker of the House</string>
    <string name="quiz_6_answer_4">The Attorney General</string>
    <string name="quiz_6_answer_5">The Chief Strategist</string>
