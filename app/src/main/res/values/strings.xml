    <!-- QUIZ Question 8 selection -->
    <string name="quiz_8_question">The House of Representatives has how many voting members?</string>
    <string name="quiz_8_answer_1">Four hundred thirty-five (435)</string>
    <string name="quiz_8_answer_2">One hundred (100)</string>
    <string name="quiz_8_answer_3">Two hundred thirty-five (235)</string>
    <string name="quiz_8_answer_4">50 / 50</string>
