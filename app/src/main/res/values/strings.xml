    <string name="quiz_7_answer_10">Blackfeet</string>
    <string name="quiz_7_answer_11">Seminole</string>
    <string name="quiz_7_answer_12">Cheyenne</string>
    <string name="quiz_7_answer_13">Arawak</string>
